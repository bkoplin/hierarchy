    "@types/chroma-js": "^2.4.0",
    "@types/d3-array": "^3.0.5",
    "@types/d3-hierarchy": "^3.1.2",
    "@types/d3-scale": "^4.0.3",
    "@types/d3-shape": "^3.1.1",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.8",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.4.5",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitest/ui": "^0.33.0",
    "bumpp": "^9.1.1",
    "changelogithub": "^0.12.12",
    "chroma-js": "^2.4.2",
    "d3-array": "^3.2.4",
    "d3-hierarchy": "^3.1.2",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "esbuild-plugins-node-modules-polyfill": "^1.3.0",
    "eslint": "^8.45.0",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "lint-staged": "^13.2.3",
    "lodash-es": "^4.17.21",
    "paper": "^0.12.17",
    "pnpm": "^8.6.10",
    "query-string": "^8.1.0",
    "rambdax": "^10.0.0",
    "taze": "^0.11.2",
    "ts-toolbelt": "^9.6.0",
    "tsup": "^7.1.0",
    "type-fest": "^4.0.0",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1",
    "vite": "^4.4.7",
    "vitest": "^0.33.0",
    "yaml": "^2.3.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.0"
  }
