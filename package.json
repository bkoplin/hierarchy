{
  "name": "hierarchy",
  "version": "3.1.2",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint --ext .ts,.tsx src",
    "op:env": "op inject -i env.tpl -o .env && cat .env",
    "release": "pnpm build && pnpm bumpp && pnpm changelogithub",
    "up-deps": "taze latest -Iiu"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
